String getRepoName(){
    return scm.getUserRemoteConfigs()[0].getUrl().tokenize('/')[3].split("\\.")[0]
}
def NAME=getRepoName()
pipeline {
    agent any


    stages {
        stage('Build') {
            steps {
               script{
                    sh "echo ${getRepoName()}"
                }
               
            }
        }
    }
}
